
@{
    ViewBag.Title = "Index";
}
<div class="row">
    <div class="6u skel-cell-important">
        <section id="content">
            <header>
                <h2>Record Player Score</h2>
            </header>
        </section>
    </div>

</div>
<br />
<br />
<input type="button" onclick="location.href='@Url.Action("Logout","LogIn")'" name="ctl00$ContentPlaceHolder1$SignOut" value="Sign Out" id="ContentPlaceHolder1_SignOut" />
<br />
<br />
    Tee:  @Html.DropDownList("ContentPlaceHolder1_TeeList", ViewData["Tee"] as List<SelectListItem>)
    Course Rating:
    <input name="ctl00$ContentPlaceHolder1$Rating" type="number" id="ContentPlaceHolder1_Rating" />

        <td><input name="ctl00$ContentPlaceHolder1$S2" type="number" value="0" class="shots-btnfirst" id="ContentPlaceHolder1_S2" style="width:60px;" /></td>
        <td><input name="ctl00$ContentPlaceHolder1$S3" type="number" value="0" class="shots-btnfirst" id="ContentPlaceHolder1_S3" style="width:60px;" /></td>
